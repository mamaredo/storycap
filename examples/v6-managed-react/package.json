  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/addons": "^6.4.0",
    "@storybook/cli": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/react": "^12.1.3",
    "babel-loader": "^8.0.5",
    "rimraf": "^2.6.3"
  }
