{
  "name": "v6-managed-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^5.0.0"
  },
  "scripts": {
    "clear": "rimraf __screenshots__",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --setupFiles ./setupFile.js",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9008",
    "build-storybook": "build-storybook",
    "prestorybook": "../../scripts/e2e-prestorybook.js .",
    "storycap:all": "yarn prestorybook && yarn test && storycap --verbose --server-timeout 100000 --server-cmd \"start-storybook -p 9008\" \"http://localhost:9008\""
